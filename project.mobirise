                  },
                  ".amp-iconfont": {
                    "font-size": "60px",
                    "width": "60px",
                    "color": "@iconsColor"
                  },
                  ".number": {
                    "width": "40px",
                    "height": "40px",
                    "position": "absolute",
                    "right": "-20px",
                    "bottom": "-20px",
                    "background-color": "@iconsNumberColor",
                    "color": "contrast(@iconsNumberColor) !important",
                    "& > *": {
                      "top": "50% !important",
                      "left": "50%",
                      "transform": "translate(-50%, -50%)",
                      "position": "absolute"
                    }
                  },
                  "& > .mbr-iconfont": {
                    "position": "absolute",
                    "right": "-15px",
                    "top": "57%",
                    "font-size": "20px",
                    "width": "20px",
                    "color": "@primaryColor"
                  }
                },
