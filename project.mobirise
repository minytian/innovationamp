ess2\" min=\"0\" max=\"100\" step=\"1\" value=\"80\" condition=\"progressCount>1\">\n        <input type=\"range\" inline=\"\" title=\"Progress 3\" name=\"progress3\" min=\"0\" max=\"100\" step=\"1\" value=\"90\" condition=\"progressCount>2\">\n        <input type=\"range\" inline=\"\" title=\"Progress 4\" name=\"progress4\" min=\"0\" max=\"100\" step=\"1\" value=\"59\" condition=\"progressCount>3\">\n        <input type=\"range\" inline=\"\" title=\"Progress 5\" name=\"progress5\" min=\"0\" max=\"100\" step=\"1\" value=\"100\" condition=\"progressCount>4\">\n        <input type=\"range\" inline=\"\" title=\"Progress 6\" name=\"progress6\" min=\"0\" max=\"100\" step=\"1\" value=\"100\" condition=\"progressCount>5\">\n   \n    <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/ect25.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#404040\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n    \n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n\n        <div class=\"mbr-row mbr-jc-c\">\n            <div class=\"progress_elements mbr-col-sm-12 mbr-col-md-12\" mbr-class=\"{'mbr-col-lg-5': progressWidth == 1, 'mbr-col-lg-6': progressWidth == 2, 'mbr-col-lg-7': progressWidth == 3,'mbr-col-lg-8': progressWidth == 4, 'mbr-col-lg-10': progressWidth == 5, 'mbr-col-lg-12': progressWidth == 6}\">\n                \n        <div class=\"title__block mbr-col-sm-12 mbr-col-md-12 mbr-flex mbr-mr-1\" mbr-class=\"{'mbr-jc-c' : titleAlign == 2, 'mbr-jc-e' : titleAlign == 3}\">\n            <div class=\"item__icon rounded__10 mbr-align-center mbr-jc-c mbr-flex\">\n                <span class=\"mbr-iconfont align-center mbr-iconfont-btn fa-line-chart fa\" mbr-icon=\"\" mbr-if=\"showIcon\"></span>\n            </div>\n            <h2 mbr-if=\"showTitle\" class=\"title mbr-flex mbr-section-title mbr-fonts-style mbr-black mbr-bold mbr-align-center\" mbr-theme-style=\"display-5\" data-app-selector=\".title\">Progress</h2>\n        </div>\n                <div class=\"progress1 progressbar mbr-pt-1\" mbr-class=\"{'last-child': progressCount == 1}\">\n                    <div class=\"title-progressbar-wrap mbr-flex mbr-jc-sb\">\n                        <p class=\"progressbar-title mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" data-app-selector=\".title-progressbar-wrap .progressbar-title\">\n                           Develop</p>\n                        <p class=\"progress_value mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" data-app-selector=\".progress_value\">%</p>\n                    </div>\n                    <div class=\"progressbar-wrap\">\n                        <div class=\"progress\"></div>\n                    </div>\n                </div>\n\n                <div class=\"progress2 progressbar\" mbr-if=\"progressCount > 1\" mbr-class=\"{'last-child': progressCount == 2}\">\n                    <div class=\"title-progressbar-wrap mbr-flex mbr-jc-sb\">\n                        <p class=\"progressbar-title mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" data-app-selector=\".title-progressbar-wrap .progressbar-title\">\n                           Design</p>\n                        <p class=\"progress_value mbr-fonts-style mbr-semibold\" m